LpEsp funciones5;

Variables:
    var entero resultado;
     
Funciones:
    entero funcion2(entero x):
    
            Variables:
                var entero temporalLocal;
            Bloque:
                

                Regresa 30 + x;

    entero funcion1(entero x):
    
            Variables:
                var entero temporalLocal;
            Bloque:
                

                Regresa funcion2(10);
                


